using System;
using System.Collections.Generic;
using System.Linq;


namespace ConsoleApp52
{
    class Program
    {
        static void Main(string[] args)
        {


            int[] input = Console.ReadLine()
                .Split(' ', StringSplitOptions.RemoveEmptyEntries)
                .Select(int.Parse)
                .ToArray();

            Stack<int> numbers = new Stack<int>(input);

            string command = Console.ReadLine().ToUpper();

            while (command.ToUpper() != "END")
            {
                string[] tookens = command.Split(' ', StringSplitOptions.RemoveEmptyEntries);

                switch (tookens[0])
                {
                    case "ADD":
                        numbers.Push(int.Parse(tookens[1]));
                        numbers.Push(int.Parse(tookens[2]));
                        break;
                    case "REMOVE":
                        int count = int.Parse(tookens[1]);
                        if (numbers.Count >= int.Parse(tookens[1]))
                        {

                            for (int i = 0; i < count; i++)
                            {
                                numbers.Pop();
                            }


                        }

                        break;

                    default:
                        break;


                }

                command = Console.ReadLine().ToUpper();

            }

            Console.WriteLine($"Sum: {numbers.Sum()}");
        }


    }








}
